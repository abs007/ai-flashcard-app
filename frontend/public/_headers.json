{
    "version": 1,
    "headers": [
        {
            "source": "/(.*)\\.css$",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "text/css"
                }
            ]
        },
        {
            "source": "/(.*)\\.js$",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "text/javascript"
                }
            ]
        },
        {
            "source": "/assets/(.*)\\.css$",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "text/css"
                }
            ]
        },
        {
            "source": "/assets/(.*)\\.js$",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "text/javascript"
                }
            ]
        }
    ]
}